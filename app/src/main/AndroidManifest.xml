<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.canvasgalerija">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Canvas Galerija"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CanvasGalerija">
        <activity android:name=".activities.PaymentActivity"></activity>
        <activity android:name=".activities.AddAddressActivity" />
        <activity android:name=".activities.AddressActivity" />
        <activity android:name=".activities.CartActivity" />
        <activity android:name=".activities.ShowAllActivity" />
        <activity android:name=".activities.DetailedActivity" />
        <activity android:name=".activities.OnBoardingActivity" />
        <activity android:name=".activities.RegistrationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>